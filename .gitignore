# Gitignore for Next.js applications

# Node modules
/node_modules

# Next.js build output
/.next/
/out/

# Production build artifacts
/build/

# Debugging and logging
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Environment variables
.env
.env.local
.env.development.local
.env.production.local

# Local development and editor files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
.DS_Store
Thumbs.db

# Misc
*.log
*.tmp
*.bak